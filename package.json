{
    "name": "directus-extension-human-readable-id",
    "version": "1.0.0",
    "description": "Generate a human-readable ID for a Directus text field",
    "main": "src/index.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dwaring87/directus-extension-human-readable-id.git"
    },
    "keywords": [
        "directus",
        "directus-extension",
        "directus-custom-interface",
        "human-readable-id"
    ],
    "author": "David Waring <dev@davidwaring.net>",
    "license": "MIT",
    "directus:extension": {
        "type": "interface",
        "path": "dist/index.js",
        "source": "src/index.js",
        "host": "^v9.10.0",
        "hidden": false
    },
    "scripts": {
        "build": "npx directus-extension build",
        "deploy": "DOTENV_CONFIG_PATH=.env node scripts/deploy.js"
    },
    "devDependencies": {
        "@directus/extensions-sdk": "^9.10.0"
    },
    "bugs": {
        "url": "https://github.com/dwaring87/directus-extension-human-readable-id/issues"
    },
    "homepage": "https://github.com/dwaring87/directus-extension-human-readable-id#readme",
    "dependencies": {
        "dotenv": "^16.0.1"
    }
}
